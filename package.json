    "@testing-library/react": "12.1.2",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.3.1",
    "eslint": "7.17.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-flowtype": "5.2.0",
