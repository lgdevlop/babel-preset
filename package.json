    "eslint-plugin-standard": "4.0.1",
    "jest": "27.3.1",
    "minimist": ">=0.2.1",
    "prettier-eslint": "11.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "5.3.3"
  },
  "prettier": "@intelie/prettier-config",
  "eslintConfig": {
    "extends": "@intelie",
    "rules": {
      "no-eval": "off"
    }
